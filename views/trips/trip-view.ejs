
<div class="row" id="map"></div>

<div class="row tripContainer">
    <h2><%= trip.name%></h2>
    <p><%= trip.content %></p>
    <form method="POST" action="/trips/<%= trip._id %>">
      <input type="text" name="destname"        placeholder="name">
      <textarea type="text" name="destdesc" placeholder="Why was this place so great?"></textarea>
      <br>
      <label>Address</label>
      <input type="text" name="latitude" placeholder="Latitude" >
      <input type="text" name="longitude" placeholder="Longitude">

      <input type="submit" name="">
    </form>

</div>

<script type="text/javascript">
 var mydest = <%- JSON.stringify(destinations) %>;
 </script>

